{
    "name": "olegbodyansky/deepl-api-php-post",
    "description": "DeepL API PHP client library for the DeepL.com translation API.",
    "keywords": [
        "deepl",
        "deeply",
        "translation",
        "translate",
        "language",
        "api",
        "client",
        "english",
        "german",
        "french"
    ],
    "homepage": "https://github.com/OlegBodyansky/deepl-api-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleg Bodyansky",
            "email": "oleg.bodyansky@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "^6.3 || ^7"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^6.5 || ^7.1"
    },
    "suggest": {
        "guzzlehttp/guzzle": "You might want to install Guzzle if you want to use it as the HTTP client for DeepL"
    },
    "autoload": {
        "psr-4": {
            "OlegBodyansky\\DeepL\\Api\\": "src/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit tests"
    },
    "minimum-stability": "stable"
}
